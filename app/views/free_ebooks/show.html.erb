<div class="page-header">
  <%= link_to free_ebooks_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_free_ebook_path(@free_ebook), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show free_ebook</h1>
</div>

<dl class="dl-horizontal">
  <dt>Title:</dt>
  <dd><%= @free_ebook.title %></dd>

  <dt>Description:</dt>
  <dd><%= @free_ebook.description %></dd>

  <dt>Status:</dt>
  <dd><%= @free_ebook.status %></dd>

  <dt>User:</dt>
  <dd><%= @free_ebook.user %></dd>

</dl>
